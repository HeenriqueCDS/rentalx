{
    "openapi": "3.0.0",
    "info": {
        "title": "RentalX Documentation",
        "description": "This a rental API",
        "version": "1.0.0",
        "contact": {
            "email": "henrique@dev.com.br"
        }
    },
    "paths": {
        "/categories": {
            
            "post": {
                "tags": ["Category"],
                "sumarry": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties:": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }               
                                },
                                "example": {
                                    "name": "SUV",
                                    "description": "Huge cars"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Category already exists!"
                    }
                }
            },
            "get": {
                "tags": ["Category"],
                "sumarry": "Get categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "Sucesss",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}